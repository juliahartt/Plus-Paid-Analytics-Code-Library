SELECT  
  lead_id,
  lead_created_at,
  lead_source,
  product,
  region,
  segment,
  lead_score,
  commercial_type,
  commercial_channel,
  last_touch_adjusted_dlv_usd,
  iaf,
  last_touch_iadjusted_dlv_usd
FROM `shopify-dw.mart_commercial_optimization.dollar_lead_value_attribution`
WHERE
  lead_created_at >= '2025-01-01'
  AND is_last_touchpoint IS TRUE
